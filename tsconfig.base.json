{
	"compilerOptions": {
		"target": "ES2022",
		"lib": ["ES2022", "DOM", "DOM.Iterable"],
		"module": "ESNext",
		"moduleResolution": "Bundler",
		"jsx": "react-jsx",
		"strict": true,
		"noUncheckedIndexedAccess": true,
		"noImplicitOverride": true,
		"useUnknownInCatchVariables": true,
		"allowJs": false,
		"types": ["node"],
		"baseUrl": ".",
		"paths": {
			"@/*": ["./src/*"]
		}
	}
}
