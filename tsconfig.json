{
	"extends": "./tsconfig.base.json",
	"compilerOptions": {
		"jsx": "preserve",
		"plugins": [
			{
				"name": "next"
			}
		],
		"skipLibCheck": true,
		"noEmit": true,
		"incremental": true,
		"esModuleInterop": true,
		"resolveJsonModule": true,
		"isolatedModules": true
	},
	"include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts"],
	"exclude": ["node_modules"]
}
